{
  "version": 2,
  "routes": [
    { "src": "/", "dest": "/src/public/index.html" },

    { "src": "/login", "dest": "/src/public/login.html" },

    { "src": "/admin$", "dest": "/src/admin/index.html" },
    { "src": "/admin/(.*)", "dest": "/src/admin/$1" },

    { "src": "/order/(.*)", "dest": "/src/order/$1" },

    { "src": "/toss/(.*)", "dest": "/src/public/$1" }
  ]
}
